    <!-- Weather -->
    <string name="weather_extension_title">Kαιρός</string>
    <string name="weather_extension_description">
        Εμφανίζει τις τελευταίες πληροφορίες για τον καιρό γύρω σας, συμπεριλαμβανομένων της τρέχουσας θερμοκρασίας,
        συνθηκών, καθώς και πληροφορίες για τις προβλέψεις. Μέσω του Yahoo! Weather.
    </string>
    <string name="title_weather_settings">Ρυθμίσεις επέκτασης Καιρού</string>
    <string name="temperature_template">%1$s°</string>
    <string name="later_forecast_template">Αργότερα: %1$s</string>
    <string name="no_weather_data">Μη διαθέσιμα δεδομένα καιρού</string>
    <string name="no_location_data">Η τοποθεσία σας δεν μπορεί να προσδιοριστεί</string>

    <!-- Gmail -->
    <string name="gmail_extension_title">Gmail</string>
    <string name="gmail_extension_description">
         Εμφανίζει τον αριθμό μη αναγνωσμένων emails από το Gmail ή τα εισερχόμενα προτεραιότητας.
    </string>
    <string name="title_gmail_settings">Ρυθμίσεις επέκτασης Gmail</string>
    <plurals name="gmail_title_template">
        <item quantity="other">%1$d μη διαβασμένα</item>
    </plurals>

    <!-- Next alarm -->
    <string name="next_alarm_extension_title">Επόμενης αφύπνιση</string>
    <string name="next_alarm_extension_description">
        Εμφανίζει πληροφορίες σχετικά με την επόμενη προγραμματισμένη αφύπνιση, αν έχετε θέσει μία.
    </string>
    <string name="next_alarm">Επόμενης αφύπνιση</string>

    <!-- Calendar -->
    <string name="calendar_extension_title">Ημερολογιακή συνάντηση</string>
    <string name="calendar_extension_description">
        Εμφανίζει πληροφορίες σχετικά με την επόμενη ημερολογιακή συνάντηση, αν μία έχει προγραμματιστεί για τις επόμενες ώρες.
    </string>
    <string name="title_calendar_settings">Ρυθμίσεις επέκτασης Ημερολογίου</string>
