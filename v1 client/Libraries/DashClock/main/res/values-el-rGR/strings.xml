    <!-- Missed calls -->
    <string name="missed_calls_extension_title">Αναπάντητες κλήσεις</string>
    <string name="missed_calls_extension_description">
        Εμφανίζει τον αριθμό αναπάντητων κλήσεων και τον όνομα των καλούντων που έχασες.
    </string>
    <plurals name="missed_calls_title_template">
