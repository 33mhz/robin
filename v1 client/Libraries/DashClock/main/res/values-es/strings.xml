    <!-- Weather -->
    <string name="weather_extension_title">Clima</string>
    <string name="weather_extension_description">
        Muestra información actualizada del clima cerca de ti, incluyendo temperatura actual, condición y pronóstico.
        Información proporcionada por Yahoo! Weather.
    </string>
    <string name="title_weather_settings">Configuración de la extensión del clima</string>
    <string name="temperature_template">%1$s°</string>
    <string name="later_forecast_template">Luego: %1$s</string>
    <string name="no_weather_data">Información de clima no disponible</string>
    <string name="no_location_data">Su ubicación no pudo ser determinada</string>

    <!-- Gmail -->
    <string name="gmail_extension_title">Gmail</string>
    <string name="gmail_extension_description">
        Muestra la cantidad de mensajes sin leer de la bandeja de entrada o prioritaria de Gmail.
    </string>
    <string name="title_gmail_settings">Configuración de la extensión de Gmail</string>
    <plurals name="gmail_title_template">
        <item quantity="other">%1$d sin leer</item>
    </plurals>

    <!-- Next alarm -->
    <string name="next_alarm_extension_title">Próxima alarma</string>
    <string name="next_alarm_extension_description">
        Muestra información acerca de la próxima alarma programada, si esta existe.
    </string>
    <string name="next_alarm">Próxima alarma</string>

    <!-- Calendar -->
    <string name="calendar_extension_title">Cita del Calendario</string>
    <string name="calendar_extension_description">
        Muestra información acerca de su próxima cita del calendario, si tiene
        una programada dentro de las próximas seis horas.
    </string>
    <string name="title_calendar_settings">Configuración de la extensión del Calendario</string>
    <plurals name="calendar_template_hours">
        <item quantity="one">%1$s hr</item>
        <item quantity="other">%1$s hrs</item>
    </plurals>
    <plurals name="calendar_template_mins">
        <item quantity="other">%1$s min</item>
    </plurals>
    <string name="today">Hoy</string>
    <!--<string name="now">Now</string>-->

    <!-- Missed calls -->
    <string name="missed_calls_extension_title">Llamadas perdidas</string>
    <string name="missed_calls_extension_description">
        Muestra la cantidad de llamadas perdidas y los nombres de las personas
        que no se pudieron comunicar.
    </string>
    <plurals name="missed_calls_title_template">
        <item quantity="one">%1$s llamada perdida</item>
        <item quantity="other">%1$s llamadas perdidas</item>
    </plurals>
    <string name="missed_calls_body_template">De: %1$s</string>
    <string name="missed_calls_unknown">Desconocido</string>

    <!-- SMS + MMS -->
    <string name="sms_extension_title">Mensajes de texto</string>
    <string name="sms_extension_description">
        Muestra el número de mensajes SMS o MMS sin leer, junto con el nombre
        del emisor.
    </string>
    <plurals name="sms_title_template">
        <item quantity="one">%1$s conversación sin leer</item>
        <item quantity="other">%1$s conversaciones sin leer</item>
    </plurals>
    <string name="sms_body_template">De: %1$s</string>

    <!-- Welcome strings -->
    <string name="welcome_title">Personaliza tu DashClock</string>
    <string name="welcome_description">¿Sólo quieres ver la fecha y la hora actual?
        ¡Entonces presiona el checkbox de arriba y listo!
        Pero, ¿por qué no personalizar el DashClock con un par de <i>extensiones</i>?
        \n\nLas <b>Extensiones</b> muestran información relevante y actualizada dentro de DashClock.
        Prueba a agregar un par de extensiones debajo.
    </string>

